{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "css-prefix-order": "off",
    "disown-opener": "off",
    "compat-api/css": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "meta-charset-utf-8": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 113",
    "not firefox <= 114",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ]
}